# GitHub Personal Access Token (recommended)
# Get one from: https://github.com/settings/tokens
# Only needs 'public_repo' scope for reading public data
VITE_GITHUB_TOKEN=your_github_token_here

# Auth0 Configuration
# Create an application at https://manage.auth0.com/
# Set callback URL to: http://localhost:5173/callback, https://yourdomain.com/callback
# Enable GitHub and GitLab social connections
VITE_AUTH0_DOMAIN=your_auth0_domain_here
VITE_AUTH0_CLIENT_ID=your_auth0_client_id_here
VITE_AUTH0_AUDIENCE=your_api_identifier

# Supabase Configuration
# Create a project at https://supabase.com/
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# GitLab API Token (optional)
# Get one from: https://gitlab.com/-/profile/personal_access_tokens
# Needs 'read_api' scope
VITE_GITLAB_TOKEN=your_gitlab_token_here